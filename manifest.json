{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",

  "name": "Random Subtle Patterns",
  "description": "This extension shows a new Subtle Pattern on a new tab.",
  "version": "0.3",
  "icons": {  "16": "icons/icon16.png",
              "48": "icons/icon48.png",
              "128": "icons/icon128.png" },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  }
}
